{
    "name": "funayaki/adminlte",
    "description": "AdminLTE theme for CakePHP",
    "license": "GPL-3.0",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": "^3.3.6",
        "gourmet/knp-menu": "~0.4",
        "symfony/yaml": "4.2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0",
        "squizlabs/php_codesniffer": "~2.6.0",
        "cakephp/cakephp-codesniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "Cirici\\AdminLTE\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cirici\\AdminLTE\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests",
            "App\\": "tests/App"
        }
    }
}
