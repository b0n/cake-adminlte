{
    "name": "ciricihq/AdminLTE",
    "description": "AdminLTE theme for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": "~3.0",
        "gourmet/knp-menu": "~0.4"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Cirici\\AdminLTE\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cirici\\AdminLTE\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
