{
    "name": "ciricihq/adminlte",
    "description": "AdminLTE theme for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": "~3.2",
        "gourmet/knp-menu": "~0.4",
        "symfony/yaml": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "squizlabs/php_codesniffer": "~2.6.0",
        "ciricihq/cirici-codesniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "Cirici\\AdminLTE\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cirici\\AdminLTE\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
